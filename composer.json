{
	"name": "fisharebest/localization",
	"description": "A lightweight localization database and translation tools, with data from the CLDR, IANA, ISO, etc.",
	"type": "library",
	"keywords": [
		"localization", "locale", "language", "script", "translate", "translation", "i18n", "l10n", "cldr", "gettext"
	],
	"homepage": "https://github.com/fisharebest/localization",
	"license": "GPL-3.0-or-later",
	"authors": [{
			"name": "Greg Roach",
			"email": "greg@subaqua.co.uk",
			"role": "Developer"
	}],
	"support": {
			"issues": "https://github.com/fisharebest/localization/issues"
	},
	"require": {
		"php": ">=5.3.0"
	},
	"require-dev": {
		"ext-simplexml": "*",
		"phpstan/phpstan": "*",
		"phpstan/extension-installer": "*",
		"phpstan/phpstan-deprecation-rules": "*",
		"phpstan/phpstan-strict-rules": "*",
		"phpstan/phpstan-phpunit": "*",
		"phpunit/phpunit": "*",
		"yoast/phpunit-polyfills":  "*",
		"php-coveralls/php-coveralls": "*"
	},
	"autoload": {
		"psr-4": {
			"Fisharebest\\Localization\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Fisharebest\\LocalizationTest\\": "test/"
		}
	},
	"config": {
		"allow-plugins": {
			"phpstan/extension-installer": true
		}
	}
}
